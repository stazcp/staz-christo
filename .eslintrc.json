{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // produce error
    // "eslint:recommended",
    // "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off"
  }
}
